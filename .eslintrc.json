{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-unused-expressions": 0,
        "no-unused-vars": 0,
        "no-unused-labels": 0,
        "no-undef": 0
    },
    "extends": [
        "plugin:react/recommended"
    ]
}
